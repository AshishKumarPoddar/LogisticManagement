@model IEnumerable<LogisticManagement.Models.Contact>

@{
    ViewBag.Title = "ContactDetails";
}

<h2>ContactDetails</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ConId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CollectionOffice)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContactNo_)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ConId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CollectionOffice)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ContactNo_)
        </td>
       
    </tr>
}

</table>
